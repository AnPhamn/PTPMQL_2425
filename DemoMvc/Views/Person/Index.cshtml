@model IEnumerable<DEMOMVC.Models.Person>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CCCDID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.hoVaten)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.queQuan)
            </th>
            
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            
            <td>
                @Html.DisplayFor(modelItem => item.CCCDID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.hoVaten)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.queQuan)
            </td>
          
            <td>
                <a asp-action="Edit" asp-route-id="@item.CCCDID">Edit</a> |
                <a asp-action="Delete" asp-route-id="@item.CCCDID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>